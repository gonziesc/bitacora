  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/pdep-lunes/bitacora/issues"
  },
  "dependencies": {
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.33",
